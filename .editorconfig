# Bu dosya kök dosyadır
root = true

# Genel ayarlar
[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

# C# dosyaları için kurallar
[*.cs]
dotnet_sort_system_directives_first = true
dotnet_separate_import_directive_groups = true
